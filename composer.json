{
    "name": "furbyus/ur-api-utils",
    "type": "library",
    "description": "API Utils to automate Responses in the UrAPI proper version",
    "version": "1.0.0",
    "keywords": [
        "furbyus",
        "utils",
        "rest",
        "web service",
        "ur-api",
        "response"
    ],
    "homepage": "https://github.com/furbyus/ur-api-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "Francesc Aguilar",
            "email": "furbyus@gmail.com",
            "homepage": "https://github.com/furbyus"
        }
    ],
    "require": {
        "illuminate/container": "^5.8",
        "illuminate/contracts": "^5.7",
        "illuminate/http": "^5.7",
        "illuminate/support": "^5.7",
        "symfony/http-foundation": "^4.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "UrApi\\Utils\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "UrApi\\Utils\\LaravelServiceProvider"
            ],
            "aliases": {}
        }, 
        "branch-alias": {
            "stable-master": "1.0.0-stable"
        }
        
    }
}
